package arrays;

import java.util.Scanner;

public class Desafio {
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		System.out.print("Quantas notas: ");
		
		int qtNotas = entrada.nextInt();
				
		System.out.println(qtNotas);
		
	    entrada.close(); 
	}
}
